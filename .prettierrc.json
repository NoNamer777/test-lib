{
    "$schema": "https://json.schemastore.org/prettierrc",
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "overrides": [],
    "plugins": ["prettier-plugin-organize-imports"],
    "printWidth": 120,
    "proseWrap": "never",
    "quoteProps": "consistent",
    "semi": true,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
}
